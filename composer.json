{
    "name": "mepihindeveloper/php-service-locator",
    "description": "Компонент-контейнер для работы с экземплярами классов-служб",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "component",
        "class",
        "php",
        "container",
        "psr7",
        "service locator"
    ],
    "authors": [
        {
            "name": "mepihindeveloper",
            "email": "mepihindeveloper@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "mepihindeveloper\\components\\container\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    }
}
